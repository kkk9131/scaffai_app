else
  export NODE_PATH="/Users/kazuto/Desktop/scaffai-unified/node_modules/.pnpm/expo@53.0.9_@babel+core@7.27.4_@expo+metro-runtime@5.0.4_react-native@0.79.2_@babel+core@7.27_opbxkmvz24dt244cuwl2mldehe/node_modules/expo/bin/node_modules:/Users/kazuto/Desktop/scaffai-unified/node_modules/.pnpm/expo@53.0.9_@babel+core@7.27.4_@expo+metro-runtime@5.0.4_react-native@0.79.2_@babel+core@7.27_opbxkmvz24dt244cuwl2mldehe/node_modules/expo/node_modules:/Users/kazuto/Desktop/scaffai-unified/node_modules/.pnpm/expo@53.0.9_@babel+core@7.27.4_@expo+metro-runtime@5.0.4_react-native@0.79.2_@babel+core@7.27_opbxkmvz24dt244cuwl2mldehe/node_modules:/Users/kazuto/Desktop/scaffai-unified/node_modules/.pnpm/node_modules:$NODE_PATH"
